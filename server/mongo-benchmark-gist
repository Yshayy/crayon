// Run with `mongo test foo.js`
use crayon_tests;
db.foos.drop();
var docs = [];
for(var i = 0; i < 50000; i++) {
  docs.push({"_id":i, "t": "2013-06-23", "N":7*i, "A":1, "V":0, "n":"1234"});
}
 
var t0 = Date.now();
db.foos.insert(docs);
//db.getLastError(); // w: 1
var t1 = Date.now();
 
print(db.foos.find().count() + " documents inserted");
print("Took " + (t1 - t0) + "ms");
